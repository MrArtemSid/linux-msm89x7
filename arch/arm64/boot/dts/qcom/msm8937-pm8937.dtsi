#include "msm8937.dtsi"
#include "pm8937.dtsi"

&dsi0 {
	vdd-supply = <&pm8937_l17>;
	vdda-supply = <&pm8937_l2>;
	vddio-supply = <&pm8937_l6>;
};

&dsi_phy0 {
	vddio-supply = <&pm8937_l6>;
};

&sdhc_1 {
	vmmc-supply = <&pm8937_l8>;
	vqmmc-supply = <&pm8937_l5>;
};

&sdhc_2 {
	vmmc-supply = <&pm8937_l11>;
	vqmmc-supply = <&pm8937_l12>;
};

&cpr {
	vdd-apc-supply = <&vreg_apc>;
	vdd-apc-step-uv = <5000>;
};

&rpm_requests {
	smd_rpm_regulators: pm8937-regulators {
		compatible = "qcom,rpm-pm8937-regulators";

		pm8937_s1: s1 {};
		/* pm8937_s2 is managed by rpmpd (MSM8937_VDDCX) */
		pm8937_s3: s3 {};
		pm8937_s4: s4 {};

		pm8937_l1: l1 {};
		pm8937_l2: l2 {};
		/* pm8937_l3 is managed by rpmpd (MSM8937_VDDMX) */
		pm8937_l4: l4 {};
		pm8937_l5: l5 {};
		pm8937_l6: l6 {};
		pm8937_l7: l7 {};
		pm8937_l8: l8 {};
		pm8937_l9: l9 {};
		pm8937_l10: l10 {};
		pm8937_l11: l11 {};
		pm8937_l12: l12 {};
		pm8937_l13: l13 {};
		pm8937_l14: l14 {};
		pm8937_l15: l15 {};
		pm8937_l16: l16 {};
		pm8937_l17: l17 {};
		pm8937_l18: l18 {};
	};
};
